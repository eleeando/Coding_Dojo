@{
    ViewData["Title"] = "Home Page";
}
@model List<Dishes>
<div class="text-center">
    <h1 class="display-4">🍜🍘🍙 Welcome To CRUDelicious 🍙🍘🍜</h1>
    <div class="card shadow border-warning p-3">
        <div class="card-head h3 text-center">🍜Check out some recent dishes🍜</div>
            <div class="card-body">
                @foreach (Dishes item in Model)
                {
                    <p><a href="/dishes/@item.DishId">@item.Name</a> by @item.Chef</p> 
                }
            </div>
    </div>
    
</div>
